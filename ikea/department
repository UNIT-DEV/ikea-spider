		<script type="text/javascript">
			var startUrl = "\x2Fcn\x2Fzh\x2Fcatalog\x2Fallproducts\x2Fdepartment\x2Fdepartment\x2F";
			
			
			if(startUrl.contains("allproducts")){
			    
			    if(startUrl.contains("department"))
			    {
			        if(startUrl.contains("department/")){
			            
			            startUrl= startUrl.substr(0,37); 
		          
			        }else{
			            startUrl= startUrl.substr(0,36); 
			          
			        }
			    }
			    
			    if(startUrl.contains("alphabetical") && !startUrl.contains("department"))
			    {
			        if(startUrl.contains("alphabetical/")){
			            
			            startUrl= startUrl.substr(0,39); 
		          
			        }else{
			            startUrl= startUrl.substr(0,38); 
			          
			        }
			    }
			    if(!startUrl.contains("alphabetical") && !startUrl.contains("department"))
			    {
			        if(startUrl.contains("allproducts")){
			            
			            startUrl= startUrl.substr(0,26); 
		          
			        }
			    }
			    
			}
			
			if(!startUrl.endsWith("/")) startUrl = startUrl + "/";
			if(startUrl.endsWith("department/")) startUrl = startUrl.substr(0,startUrl.length-11);
			if(startUrl.endsWith("alphabetical/")) startUrl = startUrl.substr(0,startUrl.length-13);
			var optionUrls = [startUrl,startUrl+"department/",startUrl+"alphabetical/"];
		</script>
					
